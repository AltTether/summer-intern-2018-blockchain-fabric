<!DOCTYPE html>
<html>
  <head>
    <title>KAWAYA</title>
  </head>
  <body>
    <h1>KAWAYA Rooms</h1>
    <ul>
      <% for (var i = 0; i < rooms.length; i++) { %>
      <li>
        <p><%= rooms[i].id %></p>
        <% if (rooms[i].status_of_use == 'notUsed') { %>
          <img src="./static/wc_img.jpg" alt="">
          <p>使用状況: 空室</p>
          <form method="post">

            <input type="hidden" name="room_id" value="<%= rooms[i].id %>">
            <input type="hidden" name="user_hash" value="560049c49ddb32fd83a0a137074856df">
            <input type="submit" value="予約">
          </form>
        <% } else { %>
          <p>使用状況: 使用中</p>
        <% } %>
        <br>
      </li>
      <% } %>
    </ul>
  </body>
</html>
